{
  "manifest_version": 3,
  "name": "Web FSR",
  "version": "1.0.0",
  "description": "FidelityFX Super Resolution 1.0 for Web mediasource",
  "developer": {
    "name": "Hajime-san",
    "url": "https://github.com/Hajime-san/web-fsr"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["assets/index.js"],
      "run_at": "document_start"
    }
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "128": "icon128.png"
  }
}
